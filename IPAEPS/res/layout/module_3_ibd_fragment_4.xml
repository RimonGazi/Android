<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linear_layout_ibd_2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_1"
    android:orientation="vertical"
    android:paddingLeft="@dimen/start_page_padding_lefr_right"
    android:paddingRight="@dimen/start_page_padding_lefr_right"
    android:paddingTop="@dimen/start_page_content_padding_top" >

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <com.rimon.rsa.ipaeps.textview.AnyTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                android:gravity="center|left"
                android:text="@string/q_module_3_13"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/black"
                android:textSize="@dimen/btn_text_size" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="0dip" >

                <RadioGroup
                    android:id="@+id/rg_q_module_3_13"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:orientation="vertical" >

                    <com.rimon.rsa.ipaeps.radiobutton.AnyRadioButton
                        android:id="@+id/radio29"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:button="@null"
                        android:drawableLeft="@drawable/check_box_style"
                        android:drawablePadding="@dimen/checnox_drawable_padding_left"
                        android:text="@string/yes_tem"
                        android:textColor="@color/check_box_state_1"
                        android:textSize="@dimen/radio_text_size_2" />

                    <com.rimon.rsa.ipaeps.radiobutton.AnyRadioButton
                        android:id="@+id/radio30"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/checkbox_margin_top_button"
                        android:button="@null"
                        android:drawableLeft="@drawable/check_box_style"
                        android:drawablePadding="@dimen/checnox_drawable_padding_left"
                        android:text="@string/yes_permanent"
                        android:textColor="@color/check_box_state_1"
                        android:textSize="@dimen/radio_text_size_2" />

                    <com.rimon.rsa.ipaeps.radiobutton.AnyRadioButton
                        android:id="@+id/radio31"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/checkbox_margin_top_button"
                        android:button="@null"
                        android:drawableLeft="@drawable/check_box_style"
                        android:drawablePadding="@dimen/checnox_drawable_padding_left"
                        android:text="@string/no_if"
                        android:textColor="@color/check_box_state_1"
                        android:textSize="@dimen/radio_text_size_2" />
                </RadioGroup>
            </LinearLayout>

            <com.rimon.rsa.ipaeps.button.AnyButton
                android:id="@+id/btn_q_module_3_14"
                style="?android:attr/borderlessButtonStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                android:background="@drawable/btn_left_start_state"
                android:drawableRight="@drawable/btn_right_start_state"
                android:enabled="false"
                android:gravity="left|center_vertical"
                android:paddingBottom="@dimen/btn_text_padding_t_b"
                android:paddingLeft="@dimen/btn_text_padding_l_r"
                android:paddingTop="@dimen/btn_text_padding_t_b"
                android:text="@string/q_module_3_14"
                android:textColor="@color/color_state_1"
                android:textSize="@dimen/btn_text_size" />

            <com.rimon.rsa.ipaeps.textview.AnyTextView
                android:id="@+id/tv_q_module_3_15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                android:enabled="false"
                android:gravity="center|left"
                android:text="@string/q_module_3_15"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/color_state_1"
                android:textSize="@dimen/btn_text_size" />

            <RadioGroup
                android:id="@+id/rg_q_module_3_15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                android:gravity="center|left"
                android:orientation="vertical" >

                <com.rimon.rsa.ipaeps.radiobutton.AnyRadioButton
                    android:id="@+id/radio3_15_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@null"
                    android:drawableLeft="@drawable/check_box_style"
                    android:drawablePadding="@dimen/checnox_drawable_padding_left"
                    android:enabled="false"
                    android:text="@string/colostomy"
                    android:textColor="@color/check_box_state_1"
                    android:textSize="@dimen/radio_text_size_2" />

                <com.rimon.rsa.ipaeps.radiobutton.AnyRadioButton
                    android:id="@+id/radio3_15_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/checkbox_margin_top_button"
                    android:button="@null"
                    android:drawableLeft="@drawable/check_box_style"
                    android:drawablePadding="@dimen/checnox_drawable_padding_left"
                    android:enabled="false"
                    android:text="@string/ileostomy"
                    android:textColor="@color/check_box_state_1"
                    android:textSize="@dimen/radio_text_size_2" />

                <com.rimon.rsa.ipaeps.radiobutton.AnyRadioButton
                    android:id="@+id/radio3_15_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/checkbox_margin_top_button"
                    android:button="@null"
                    android:drawableLeft="@drawable/check_box_style"
                    android:drawablePadding="@dimen/checnox_drawable_padding_left"
                    android:enabled="false"
                    android:text="@string/urostomy"
                    android:textColor="@color/check_box_state_1"
                    android:textSize="@dimen/radio_text_size_2" />
            </RadioGroup>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical" >

                <com.rimon.rsa.ipaeps.textview.AnyTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                    android:gravity="center|left"
                    android:text="@string/q_module_3_16"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="@color/black"
                    android:textSize="@dimen/btn_text_size" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:padding="0dip" >

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/start_btn_mrgin_l_r_t"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:padding="0dip" >

                        <CheckBox
                            android:id="@+id/radio32"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:button="@null"
                            android:drawableLeft="@drawable/check_box_style"
                            android:drawablePadding="@dimen/checnox_drawable_padding_left"
                            android:text="@string/irritable_bowel"
                            android:textColor="@color/check_box_state_1"
                            android:textSize="@dimen/radio_text_size_2" />

                        <CheckBox
                            android:id="@+id/radio33"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/checkbox_margin_top_button"
                            android:button="@null"
                            android:drawableLeft="@drawable/check_box_style"
                            android:drawablePadding="@dimen/checnox_drawable_padding_left"
                            android:text="@string/chronic_fatigue"
                            android:textColor="@color/check_box_state_1"
                            android:textSize="@dimen/radio_text_size_2" />

                        <CheckBox
                            android:id="@+id/radio34"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/checkbox_margin_top_button"
                            android:button="@null"
                            android:drawableLeft="@drawable/check_box_style"
                            android:drawablePadding="@dimen/checnox_drawable_padding_left"
                            android:text="@string/fibromyalgia"
                            android:textColor="@color/check_box_state_1"
                            android:textSize="@dimen/radio_text_size_2" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/start_btn_mrgin_l_r_t"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:padding="0dip" >

                        <CheckBox
                            android:id="@+id/radio35"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:button="@null"
                            android:drawableLeft="@drawable/check_box_style"
                            android:drawablePadding="@dimen/checnox_drawable_padding_left"
                            android:text="@string/primary_sclerosing"
                            android:textColor="@color/check_box_state_1"
                            android:textSize="@dimen/radio_text_size_2" />

                        <CheckBox
                            android:id="@+id/radio36"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/checkbox_margin_top_button"
                            android:button="@null"
                            android:drawableLeft="@drawable/check_box_style"
                            android:drawablePadding="@dimen/checnox_drawable_padding_left"
                            android:text="@string/ankylosing_spondylitis"
                            android:textColor="@color/check_box_state_1"
                            android:textSize="@dimen/radio_text_size_2" />

                        <CheckBox
                            android:id="@+id/radio37"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/checkbox_margin_top_button"
                            android:button="@null"
                            android:drawableLeft="@drawable/check_box_style"
                            android:drawablePadding="@dimen/checnox_drawable_padding_left"
                            android:text="@string/other"
                            android:textColor="@color/check_box_state_1"
                            android:textSize="@dimen/radio_text_size_2" />
                    </LinearLayout>
                </LinearLayout>

                <com.rimon.rsa.ipaeps.edittext.AnyEditText
                    android:id="@+id/et_q_module_3_16"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edit_text_height"
                    android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                    android:background="@drawable/btn_bacground"
                    android:ems="10"
                    android:enabled="false"
                    android:gravity="left|center_vertical"
                    android:hint="@string/other"
                    android:inputType="text"
                    android:paddingBottom="@dimen/start_btn_padding_top_buttom"
                    android:paddingLeft="@dimen/start_btn_padding_l_r"
                    android:paddingTop="@dimen/start_btn_padding_top_buttom"
                    android:textSize="@dimen/start_page_text_size" >
                </com.rimon.rsa.ipaeps.edittext.AnyEditText>

                <include
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                    layout="@layout/btn_previous" />

                <include
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/start_btn_mrgin_l_r_t"
                    layout="@layout/btn_next" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>